{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "branch": {
      "type": "string",
      "pattern": "^[0-9]{3}$"
    },
    "currencyCode": {
      "type": "string",
      "pattern": "^[A-Z]{3}$"
    },
    "flexcubeCustomerId": {
      "type": "string",
      "pattern": "^[0-9A-Za-z]{9}$"
    },
    "customerId": {
      "type": "string"
    },
    "dealId": {
      "type": "string"
    },
    "customerType": {
      "type": "string"
    }
  },
  "required": [
    "currencyCode",
    "flexcubeCustomerId"
  ]
}