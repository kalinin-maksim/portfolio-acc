{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "accountingId": {
      "type": "string"
    },
    "msgType": {
      "type": "string"
    },
    "moduleId": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "createdDate": {
      "type": "string",
      "pattern": "\\d{4}-(0[1-9]|1[0-2])-(0[1-9]|1[0-9]|2[0-9]|3[0-1])T(?:[01]\\d|2[0123]):([012345]\\d):([012345]\\d)((\\.\\d{1,6})?(([+\\-]\\d{2}:\\d{2})|Z)?)"
    },
    "msgCode": {
      "type": "number"
    },
    "description": {
      "type": "string"
    }
  },
  "required": [
    "accountingId",
    "msgType",
    "moduleId",
    "createdDate",
    "msgCode",
    "status",
    "description"
  ]
}