<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

    <changeSet id="20231101-create_account_class_table"
               author="---">

        <createTable tableName="${prefix}account_class"
                     remarks="account class">
            <column name="id"
                    type="${uuid_type}"
                    remarks="Unique ID generated by the system">
                <constraints primaryKeyName="pk_account_class_id"
                             primaryKey="true"
                             nullable="false"/>
            </column>
            <column name="sign" type="VARCHAR(1)"/>
        </createTable>

    </changeSet>

</databaseChangeLog>